{
  "Dhcp4": {
    "interfaces-config": {
      "interfaces": ["ens4"]
    },
    "control-socket": {
      "socket-type": "unix",
      "socket-name": "/run/kea/kea4-ctrl-socket"
    },
    "lease-database": {
      "type": "memfile",
      "lfc-interval": 3600
    },
    "valid-lifetime": 600,
    "max-valid-lifetime": 7200,
    "subnet4": [
      {
        "id": 10,
        "subnet": "192.168.10.0/24",
        "pools": [
          {
            "pool": "192.168.10.50 - 192.168.10.200"
          }
        ],
        "option-data": [
          {
            "name": "routers",
            "data": "192.168.10.1"
          },
          {
            "name": "domain-name",
            "data": "examples.management"
          }
        ]
      },
      {
        "id": 100,
        "subnet": "192.168.100.0/24",
        "pools": [
          {
            "pool": "192.168.100.50 - 192.168.100.200"
          }
        ],
        "option-data": [
          {
            "name": "routers",
            "data": "192.168.100.1"
          },
          {
            "name": "domain-name",
            "data": "examples.it-area"
          }
        ]
      },
      {
        "id": 200,
        "subnet": "192.168.200.0/24",
        "pools": [
          {
            "pool": "192.168.200.50 - 192.168.200.200"
          }
        ],
        "option-data": [
          {
            "name": "routers",
            "data": "192.168.200.1"
          },
          {
            "name": "domain-name",
            "data": "examples.dc-pcs"
          }
        ]
      }
    ]
  }
}
